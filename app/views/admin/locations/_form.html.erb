        <%= f.text_field :contact_email, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :contact_phone %><br />
        <%= f.text_field :contact_phone, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :contact_fax %><br />
        <%= f.text_field :contact_fax, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :link_facebook %><br />
        <%= f.text_field :link_facebook, class: 'form-control' %>
      </div>

      <div class="field">
