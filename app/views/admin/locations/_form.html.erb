        <%= f.text_field :address_street, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :address_street2 %><br />
        <%= f.text_field :address_street2, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :address_city %><br />
        <%= f.text_field :address_city, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :address_state %><br />
        <%= f.text_field :address_state, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :address_zip %><br />
        <%= f.text_field :address_zip, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :drive_through_drive_up %><br />
        <%= f.check_box :drive_through_drive_up, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :takeout %><br />
        <%= f.check_box :takeout, class: 'form-control' %>
      </div>

      <div class="field">
        <%= f.label :delivery %><br />
        <%= f.check_box :delivery, class: 'form-control' %>
      </div>
    </div>

    <div class="col-md-4">

    </div>
  </div>


  <div class="actions">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
